from selenium import webdriver
from selenium.webdriver.support.ui import Select

driver = webdriver.Chrome(executable_path="D:\Tools\Python\Driver\Chrome\chromedriver.exe")

driver.get("https://mat.aima.in/sep20/register-online")

#Select one dropdown option
drpdwn1=driver.find_element_by_id("regis-date")
drp1=Select(drpdwn1)
drp1.select_by_visible_text("14")

drpdwn2=driver.find_element_by_id("regis-month")
drp2=Select(drpdwn2)
drp2.select_by_index("9")

drpdwn3=driver.find_element_by_id("regis-year")
drp3=Select(drpdwn3)
drp3.select_by_value("1988")

#Count no of options
drpdwn4=driver.find_element_by_id("state_ut")
drp4=Select(drpdwn4)
print(len(drp4.options))

#Print all the options in dropdown
all_options=drp4.options
for option in all_options:
    print(option.text)

driver.close()
